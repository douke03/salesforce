<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Checked</fullName>
    <active>true</active>
    <errorConditionFormula>IF(OR(ISCHANGED(Select__c),ISCHANGED(Date__c)),CheckBox__c,FALSE)</errorConditionFormula>
    <errorMessage>チェックボックスがTRUEの場合は変更不可</errorMessage>
</ValidationRule>
