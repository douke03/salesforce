<!-- Keep account selected in list view -->
<apex:page standardController="Account" tabStyle="Account" recordSetVar="Accounts">
    <apex:form >
        <apex:repeat value="{!Accounts}" var="Account" rendered="true">
            <p>{!Account.Id} {!Account.OwnerId}</p>
        </apex:repeat>
        <apex:commandButton value="Go" action="{!list}"/>
    </apex:form>
</apex:page>